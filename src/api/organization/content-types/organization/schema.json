{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "singularName": "organization",
    "pluralName": "organizations",
    "displayName": "Organization"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "city": {
      "type": "string"
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "coverImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "slogan": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "quizzes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "organization"
    }
  }
}
